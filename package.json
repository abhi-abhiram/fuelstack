{
    "name": "fuelstack",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev --no-cache --continue --concurrency=20",
        "deps": "npx --yes npm-check-updates -x typescript --deep --interactive",
        "format": "prettier --write \"**/*.{json,css,cjs,mjs,js,jsx,ts,tsx,md,mdx}\"",
        "clean": "turbo clean",
        "lint": "turbo lint",
        "test": "turbo test"
    },
    "devDependencies": {
        "@acme/prettier-config": "workspace:^",
        "prettier-plugin-tailwindcss": "^0.4.1",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.1",
        "turbo": "^1.10.7"
    },
    "prettier": "@acme/prettier-config",
    "packageManager": "pnpm@8.6.7"
}
